{
  "family": "rpc-task-definition", 
  "executionRoleArn": "arn:aws:iam::177660332892:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc", 
  "containerDefinitions": [
      {
          "name": "rpc-app", 
          "image": "177660332892.dkr.ecr.us-west-2.amazonaws.com/rpc-repository:latestt", 
          "portMappings": [
              {
                  "containerPort": 80, 
                  "hostPort": 80, 
                  "protocol": "tcp"
              }
          ], 
          "essential": true
      }
  ], 
  "requiresCompatibilities": [
      "FARGATE"
  ], 
  "cpu": "256", 
  "memory": "512"
}